    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <title>Bon.ker Casting</title>

</head>
<body>

    <hr />
    <h1 style="color:#375F1B; text-align:center; font-size:70px; "><img src="~/Images/Company_Logo.png" height="70px" width="70px" border="1px">Bon. ker Casting</h1>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <nav class="navbar navbar-inverse">
            <div class="container-fluid">
                @*<div class="navbar-header">
                        <a class="navbar-brand" href="#">Logo</a>
                    </div>*@
                <div class="navbar-collapse collapse" id="myNavbar">
                    <ul class="nav navbar-nav">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="@Url.Action("Login","Home")"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>

                    </ul>
                </div>
            </div>
        </nav>
    </div>

    <div id="sidebar">
        @RenderSection("Sidebar", required: false)
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        @*<footer>
                <p>&copy; Mandy Kerfoot</p>
            </footer>*@
